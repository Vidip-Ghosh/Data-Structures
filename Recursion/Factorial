#include<iostream>
using namespace std;
int Factorial(int n)
{
    if(n==0)
    {
        return 1;
    }
    else
    {
        return n*Factorial(n-1);
    }
}

int main()
{
    int n;
    cout<<"Enter a number: "<<endl;
    cin>>n;
    int result = Factorial(n);
    cout<<"Factorial of a number is:"<<result;
}
