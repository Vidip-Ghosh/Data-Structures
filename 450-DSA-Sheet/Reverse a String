#include<bits/stdc++.h>
using namespace std;

string reverseWord(string str){
  int left=0;
  int right=str.length()-1;
  string ans = "";
  while(left<=right)
  {
      ans += str[right];
      right--;
  }
  return ans;
}

int main() {
	
	int t;
	cin>>t;
	while(t--)
	{
	string s;
	cin >> s;
	
	cout << reverseWord(s) << endl;
	}
	return 0;
	
}
