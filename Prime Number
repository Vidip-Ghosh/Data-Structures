#include<iostream>
using namespace std;

int main()
{
    int n;
    cout<<"Enter the value of n: ";
    cin>>n;
    bool isPrime = 1;
    for(int i=2;i<n;i++)
    {
        if(n%i == 0)    //rem=0-> Not a prime number
        {
            // cout<<"Not a prime number"<<endl;
            isPrime=0;
            break;
        }
        // else
        // {
        //     cout<<"Prime number"<<endl;
        // }
    }
    if(isPrime==0)
    {
        cout<<"Not a Prime number"<<endl;
    }
    else
    {
        cout<<"Prime number"<<endl;
    }
}
