#include<iostream>
#include<cstring>
using namespace std;

bool Check(string s)
{
    for(int i=0;i<s.length();i++)
    {
        if(s[i]!=s[0])
        {
            return false;
        }
    }
    return true;
}

int main()
{
    string s = "777";
    if(Check(s)==true)
    {
        printf("All characters are same");
        return 1;
    }
    else
    {
        printf("Not same characters");
        return 0;
    }
}
